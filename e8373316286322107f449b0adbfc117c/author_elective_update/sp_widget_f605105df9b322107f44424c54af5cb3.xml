        <template><![CDATA[<button class="btn btn-primary" ng-click="toggleResidents()">
  Show Residents
</button>

<div ng-if="showResidents">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Flat</th>
        <th>Phone</th>
      </tr>
    </thead>
    <tbody>
      <tr ng-repeat="resident in data.residents">
        <td>{{ resident.name }}</td>
        <td>{{ resident.flat }}</td>
        <td>{{ resident.phone }}</td>
      </tr>
    </tbody>
  </table>
</div>
]]></template>
