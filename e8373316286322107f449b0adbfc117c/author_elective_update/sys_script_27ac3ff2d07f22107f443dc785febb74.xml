        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
  var base = gs.getProperty('glide.servlet.uri');
  var spLink = base + 'sp?id=record_view&table=u_community_maintenance_task&sys_id=' + current.getUniqueValue();

  // Condition: Resident exists
  if (!current.u_resident.nil()) {

    // 1) New task submitted (insert)
    if (current.operation() == 'insert') {
      gs.eventQueue('x_scope.cmt.task.resident_notify', current, current.getUniqueValue(), spLink);
    }

    // 2) Task assigned (assigned_to just set or changed)
    var assigneeJustSet = previous && previous.assigned_to.nil() && !current.assigned_to.nil();
    var assigneeChanged = previous && !previous.assigned_to.nil() && current.assigned_to.changes();

    if (current.operation() == 'update' && (assigneeJustSet || assigneeChanged)) {
      gs.eventQueue('x_scope.cmt.task.resident_notify', current, current.getUniqueValue(), spLink);
    }
  }
})(current, previous);]]></script>
